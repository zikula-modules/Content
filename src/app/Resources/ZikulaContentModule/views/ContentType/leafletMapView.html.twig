<div class="content-leaflet hidden" data-mapid="map{{ contentId }}" data-latitude="{{ latitude }}" data-longitude="{{ latitude }}" data-zoom="{{ zoom }}" data-descriptionid="text{{ contentId }}" data-tile-layer-url="{{ tileLayerUrl|e('html_attr') }}" data-tile-layer-attribution="{{ tileLayerAttribution|e('html_attr') }}">
    <div id="map{{ contentId }}" class="map" style="width: 100%; height: {{ height }}px"></div>
    <div id="text{{ contentId }}">{{ text|safeHtml }}</div>
</div>
